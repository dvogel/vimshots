filetype plugin indent on
syntax on
syntax sync fromstart
colorscheme vimshots
set tabstop=2
set shiftwidth=2
set linespace=4
set noshowmatch
set nohlsearch
set nowrap
set guioptions=Pegit
set guifont=Consolas\ 11
set shortmess=atTWA
set laststatus=0
set noruler
set number
set columns=9999
set lines=9999
